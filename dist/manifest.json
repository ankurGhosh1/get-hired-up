{
  "manifest_version": 3,
  "name": "4dayweek Job Search",
  "description": "4dayweek search extension.",
  "version": "1.0.0",
  "icons": {
    "16": "hiring.png",
    "32": "hiring.png",
    "48": "hiring.png"
  },
  "action": {
    "default_title": "Look for new jobs in 4dayweek.io",
    "default_popup": "index.html"
  },
  "host_permissions": ["https://4dayweek.io/api"]
}
